{
  "name": "rex-action",
  "version": "1.7.0",
  "main": "./lib/index.js",
  "rex": {
    "bundleAll": true
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "lines": 60
      }
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!rex-)"
    ]
  },
  "dependencies": {
    "debug": "^2.6.3",
    "empty": "^0.10.1",
    "escape-regexp": "0.0.1",
    "invariant": "^2.1.1",
    "qs": "^6.4.0",
    "react-stylesheet": "*",
    "prop-types": "^15.0.0"
  }
}
